{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "CreateRating_Proxy": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/ratings"
      },
      "backendUri": "https://localhost/api/CreateRating"
    },
    "GetRating_Proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/ratings{id}"
      },
      "backendUri": "https://localhost/api/GetRating"
    },
    "GetRatings_Proxy": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/ratings/{user_id}"
      },
      "backendUri": "https://localhost/api/GetRatings"
    }
  }
}
