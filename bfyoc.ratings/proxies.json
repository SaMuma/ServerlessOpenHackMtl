{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "CreateRating_Proxy": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/ratings"
      },
      "backendUri": "https://localhost/api/CreateRating"
    },
    "GetRating_Proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/ratings{id}"
      },
      "backendUri": "https://localhost/api/GetRating"
    },
    "GetRatings_Proxy": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/ratings/{userId}"
      },
      "backendUri": "https://localhost/api/GetRatings"
    },
    "GetUser_Proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/users"
      },
      "backendUri": "https://serverlessohuser.trafficmanager.net/api/GetUser"
    },
    "GetProduct_Proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/product/{productId}"
      },
      "backendUri": "https://serverlessohproduct.trafficmanager.net/api/GetProduct"
    },
    "GetProducts_Proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/products"
      },
      "backendUri": "https://serverlessohproduct.trafficmanager.net/api/GetProducts"
    }
  }
}
